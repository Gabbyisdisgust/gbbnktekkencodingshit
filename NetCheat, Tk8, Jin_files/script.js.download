function rplc(val) {
return val.replace(/%0/g, "</font>").replace(/%1/g, "<font style='color: #888; font-size: 12px;'>").replace(/%2/g, "<font style='color: #444; font-size: 14px;'>").replace(/%3/g, "").replace(/%4/g, "").replace(/%5/g, "").replace(/%6/g, "").replace(/%7/g, "").replace(/%8/g, "").replace(/%9/g, "")
}

function vse(game) {
for (var i = 0; i < document.getElementsByClassName(game).length; i++) {
	document.getElementsByClassName(game)[i].style.position = "inherit";
	}
n1 = location.search.substring(1)[0]*1
n2 = location.search.substring(1)[1]*1
n3 = location.search.substring(1)[2]*1
n4 = location.search.substring(1)[3]*1
n5 = location.search.substring(1)[4]*1
n6 = location.search.substring(1)[5]*1
n7 = location.search.substring(1)[6]*1
n8 = location.search.substring(1)[7]*1
na = (n1+1)*(n2+1)*(n3+1)*(n4+1)*(n5+1)*(n6+1)*(n7+1)*(n8+1)
nal = ((na+"0").replace(/0/g, "")) * (new Date().getDate()) * (new Date().getHours()+1) + ""
if (nal.length == 1) {nall = "000"+nal}
if (nal.length == 2) {nall = "00"+nal}
if (nal.length == 3) {nall = "0"+nal}
if (nal.length >= 4) {nall = nal[0]+nal[1]+nal[2]+nal[3]}
if (location.search.substring(1) == ""+n1+n2+n3+n4+n5+n6+n7+n8+nall && location.search.substring(1) != "000000000001") {
	for (var i = 0; i < document.getElementsByClassName(game+"a").length; i++) {
		document.getElementsByClassName(game+"a")[i].style.position = "inherit";
		}
	}
try {document.getElementsByClassName("krut")[0].style.display = "none"} catch(e){}
try {document.getElementsByClassName("krut")[1].style.display = "none"} catch(e){}
try {document.getElementsByClassName("krut")[2].style.display = "none"} catch(e){}
}
